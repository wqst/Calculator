{
  "manifest_version": 2,
  "name": "Tweet Blocker",
  "version": "2.4.2",
  "description": "A simple extension to block the tweets you don't want to see on twitter",
  "content_scripts": [ {
    "matches": [ "http://twitter.com/*", "https://twitter.com/*" ],
    "css": [
    "Block/Blocker.css"
      ],
    "js": [ 
      "lib/jquery/jquery-1.7.1.min.js",
      "lib/jquery/jquery-ui-1.8.16.custom.min.js",
      "lib/tipsy/tipsy.js", 
      "lib/ko/knockout-2.1.0.js",
      "lib/ko/knockout.localStorage-latest.js",
      "lib/coffeekup/coffeekup.js",
      "Block/Blocker.js"
    ]
  
}
  ],
 
 "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "128": "img/inside.png"
  }
}
