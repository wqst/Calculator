{
  "manifest_version": 2,
  "name": "Open Tweet Filter",
  "version": "2.4.2",
  "description": "A easy chrome extension to block the tweets that you don't want",
  "update_url":"http://clients2.google.com/service/update2/crx",
  "content_scripts": [ {
    "css": [
      "data/js/jquery/tipsy/tipsy.css", 
      "data/css/block.css"
    ],
    "js": [ 
      "data/js/jquery/jquery-1.7.1.min.js",
      "data/js/jquery/jquery-ui-1.8.16.custom.min.js",
      "data/js/jquery/tipsy/tipsy.js", 
      "data/js/ko/knockout-2.1.0.js",
      "data/js/ko/knockout.localStorage-latest.js",
      "data/js/coffeekup/coffeekup.js",
      "data/js/block.js"
    ],
    "matches": [ "http://twitter.com/*", "https://twitter.com/*" ]
  } ],
  "web_accessible_resources": [
    "jquery/tipsy/tipsy.css",
    "jquery/tipsy/tipsy.gif"
  ],
  "icons": {
    "128": "data/img/icon.png"
  }
}
